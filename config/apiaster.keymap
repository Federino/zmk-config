                       <&macro_release &kp LCTRL >,
		       <&macro_release &kp X >;
        };

        m_pegar: m_pegar {
            compatible = "zmk,behavior-macro";
            #binding-cells = <0>;
            bindings = <&macro_press   &kp LCTRL >,
                       <&macro_press   &kp V >,
                       <&macro_release &kp LCTRL >,
		       <&macro_release &kp V >;
        };

