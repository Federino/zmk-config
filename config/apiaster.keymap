            compatible = "zmk,behavior-macro";
            #binding-cells = <0>;
            bindings = <&macro_press   &kp LCTRL >,
                       <&macro_press   &kp Z >,
                       <&macro_release &kp LCTRL >,
		       <&macro_release &kp Z >;
        };

        m_letras_que: m_letras_que {
            compatible = "zmk,behavior-macro";
            #binding-cells = <0>;
            bindings = <&macro_press   &kp Q >,
