//Â´TODO hacer que todos los macros, combos, etc sean macros de C. Queda mas limpio y facil de leer
#define COMBO(a, b, c, d, e)        \
        combo_##a {                 \
            timeout-ms    = <b>;    \
            key-positions = <c>;    \
            layers        = <d>;    \
            bindings      = <e>;    \
        };                          \

#define MACRO(a, b, c)                         \
        macro_##a: macro_##a {                 \
            compatible = "zmk,behavior-macro"; \
            #binding-cells = <b>;              \
            bindings = <c>;                    \
        };                                     \

